{
  "name": "giret-2.0",
  "version": "2.1.4",
  "description": "An attempt at a mountain.",
  "main": "server.js",
  "scripts": {
    "install-server": "npm install",
    "install-client": "cd client && npm install",
    "install-prod": "npm ci && cd client && npm ci",
    "prune-prod": "npm prune --omit=dev && cd client && npm prune --omit=dev",
    "server": "nodemon server.js",
    "client": "cd client && npm run dev",
    "dev": "concurrently --kill-others-on-fail \" npm run server\" \" npm run client\"",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "build-client": "cd client && npm run build",
    "deploy-local": "npm run build-client && npm start",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nocturnalmc/gp2.git"
  },
  "author": "PKP KKM",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/nocturnalmc/gp2/issues"
  },
  "homepage": "https://github.com/nocturnalmc/gp2#readme",
  "dependencies": {
    "async": "^3.2.4",
    "axios": "^1.3.5",
    "bcryptjs": "^2.4.3",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.2.0",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "moment": "^2.29.4",
    "mongoose": "^7.0.3",
    "nodemailer": "^6.9.1",
    "qrcode": "^1.5.1",
    "qs": "^6.11.2",
    "react-currency-format": "^1.1.0",
    "speakeasy": "^2.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.0.1",
    "cypress": "^12.9.0",
    "nodemon": "^3.0.1"
  }
}
