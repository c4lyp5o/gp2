{
  "name": "giret-2.0",
  "version": "2.0.15",
  "description": "An attempt at a mountain.",
  "main": "server.js",
  "scripts": {
    "install-server": "npm install",
    "install-client": "cd client && npm install",
    "install-prod": "npm install --omit dev && cd client && npm install --omit dev",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "dev": "concurrently --kill-others-on-fail \" npm run server\" \" npm run client\"",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "build-client": "cd client && npm run build",
    "deploy-local": "npm run build-client && npm start",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nocturnalmc/gp2.git"
  },
  "author": "",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/nocturnalmc/gp2/issues"
  },
  "homepage": "https://github.com/nocturnalmc/gp2#readme",
  "dependencies": {
    "async": "^3.2.4",
    "axios": "^1.3.4",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.12",
    "cron": "^2.2.0",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.0.3",
    "exceljs": "^4.3.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.2.0",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "moment": "^2.29.4",
    "mongoose": "^7.0.1",
    "nodemailer": "^6.9.1",
    "qrcode": "^1.5.1",
    "react-currency-format": "^1.1.0",
    "speakeasy": "^2.0.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "cypress": "^12.7.0",
    "nodemon": "^2.0.21"
  }
}
